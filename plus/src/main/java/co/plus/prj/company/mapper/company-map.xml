<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.plus.prj.company.mapper.CompanyMapper">
	
	<select id="getCompany" resultType="co.plus.prj.company.vo.CompanyVO">
		select co_url, co_name 
		from company
		where co_url = #{coUrl}
	</select>
	
	
</mapper>